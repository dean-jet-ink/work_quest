{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "chart.js": "^3.5.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "js-md5": "^0.7.3",
    "moment": "^2.29.1",
    "party-js": "^2.0.1",
    "react": "^16.14.0",
    "react-chartjs": "^1.2.0",
    "react-chartjs-2": "^3.0.4",
    "react-dates": "^21.8.0",
    "react-dom": "^16.14.0",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-slick": "^0.28.1",
    "react-with-direction": "^1.3.1",
    "slick-carousel": "^1.8.1",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.2",
    "@types/js-md5": "^0.4.3",
    "@types/react": "^17.0.11",
    "@types/react-dates": "^21.8.3",
    "@types/react-dom": "^17.0.7",
    "@types/react-infinite-scroller": "^1.2.2",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-slick": "^0.23.8",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "jest-environment-jsdom": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "majestic": "^1.8.1",
    "msw": "^0.35.0",
    "react-test-renderer": "^16.14.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^7.0.5",
    "typescript": "^4.3.3",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack --config webpack.prod.js",
    "analyze": "webpack --config webpack.dev.js --mode development",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "./src"
    ],
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|svg)$": "jest-transform-stub"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/__tests__/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/__tests__/env/setEnvVars.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
